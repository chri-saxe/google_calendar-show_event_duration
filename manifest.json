{
    "manifest_version": 3,
    "name": "Google calendar : Afficher la durée des événements",
    "description": "Permet d'afficher la durée de chaque événement sur Google agenda / calendar en appuyant sur B.",
    "version": "1.0",

    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "128": "images/logo-128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": ["https://calendar.google.com/calendar/*"]
        }
    ]
}